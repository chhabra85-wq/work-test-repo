				"source": [
					"telemetry_client = container.telemetry_client(run_id, f\"{api_name}-ingestion\", run_execution_time)\r\n",
					"\r\n",
					"table_actions = AzureTableActions(table_service_client, telemetry_client, params=params)\r\n",
					"flatten_instance = FlattenNestedJson(delimiter=\"_\")\r\n",
					"validation_engine = ValidationEngine(telemetry_client, params, datalake_client)\r\n",
					"data_type_with_underscore = api_name.replace(\"-\", \"_\")\r\n",
					"data_configs_helper = DataConfigsHelper(DataConfigs, f\"{data_type_with_underscore}_config\")\r\n",
					"\r\n",
					"token_authenticator = TokenAuthenticator(auth_endpoint, client_id, client_secret, telemetry_client)\r\n",
					"\r\n",
					"api_processor =  ApiProcessor(\r\n",
					"    spark_session, \r\n",
					"    params, \r\n",
					"    telemetry_client, \r\n",
					"    datalake_client, \r\n",
					"    table_service_client, \r\n",
					"    schema_transformer, \r\n",
					"    flatten_instance,\r\n",
					"    validation_engine,\r\n",
					"    data_configs_helper)\r\n",
					"\r\n",
					"api_retriever = ApiDataRetriever(params=params, table_actions=table_actions, telemetry_client=telemetry_client, table_service_client=table_service_client, api_processor=api_processor, token_authenticator=token_authenticator)\r\n",
